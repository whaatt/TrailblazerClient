<?xml version="1.0" encoding="utf-8"?>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="fill_parent" android:id="@+id/LinearLayout01" android:layout_height="fill_parent" android:orientation="vertical">
    <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Calibrate"
            android:id="@+id/textView" android:textSize="36dp" android:gravity="center"
            android:layout_marginTop="30dp"/>
    <TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="Set Peak (Current)" android:id="@+id/tvPeak"
                                                                                                                                                                                                                          android:layout_marginTop="25dp"
                                                                                                                                                                                                                          android:gravity="center"></TextView><SeekBar android:layout_height="wrap_content" android:layout_width="fill_parent" android:max="100" android:progress="18" android:id="@+id/sbPeak"
                                                                                                                                                                                                                                                                                                                                                                                        android:indeterminate="false"
                                                                                                                                                                                                                                                                                                                                                                                        android:layout_marginTop="10dp"></SeekBar><TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="Set Filter (Current)" android:id="@+id/tvFilter"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     android:layout_marginTop="10dp"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     android:gravity="center"></TextView><SeekBar android:layout_height="wrap_content" android:progress="60" android:layout_width="fill_parent" android:max="100" android:id="@+id/sbFilter"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       android:layout_marginTop="10dp"></SeekBar><TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="Step Timeout (Current)" android:id="@+id/tvTimeout"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      android:gravity="center"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      android:layout_marginTop="10dp"></TextView><SeekBar android:layout_height="wrap_content" android:layout_width="fill_parent" android:progress="500" android:max="1000" android:id="@+id/sbTimeout"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   android:layout_marginTop="10dp"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   android:indeterminate="false"></SeekBar>
    <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Stride Length (Current)"
            android:id="@+id/tvStride" android:layout_marginTop="10dp" android:gravity="center"/>
    <SeekBar
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/sbStride" android:layout_marginTop="10dp" android:max="400" android:progress="50"
            android:indeterminate="false"/>
    <SurfaceView android:layout_width="fill_parent" android:layout_height="256px" android:id="@+id/svHistory"
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             android:layout_marginTop="35dp"></SurfaceView>
















</LinearLayout>
